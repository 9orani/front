{
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "no-unused-vars": "error",
        "no-console": "warn",
        "no-else-return": "warn"
    }
}
